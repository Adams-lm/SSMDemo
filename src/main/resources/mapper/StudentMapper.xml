<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hznu.sys.mapper.StudentMapper">
    <select id="selectStudentById" resultType="com.hznu.sys.entity.Student" parameterType="java.lang.Integer">
        SELECT
        *
        FROM
        student
        WHERE Sno = #{Sno}
    </select>
</mapper>
